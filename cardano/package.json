{
  "name": "@everstake/wallet-sdk-cardano",
  "version": "1.1.3",
  "description": "Cardano - Everstake Wallet SDK",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run prebuild && tsup src/index.ts --format cjs,esm --dts",
    "type-check": "tsc",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "jest --passWithNoTests",
    "prebuild": "pnpm run type-check && pnpm run lint && pnpm run test"
  },
  "keywords": [
    "cardano",
    "blockchain",
    "everstake",
    "wallet sdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/everstake/wallet-sdk.git"
  },
  "author": "Everstake",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/everstake/wallet-sdk/issues"
  },
  "homepage": "https://github.com/everstake/wallet-sdk#readme",
  "pnpm": {
    "overrides": {
      "axios": ">=1.12.0",
      "ip": ">=2.0.1"
    }
  },
  "dependencies": {
    "@blockfrost/openapi": "^0.1.79",
    "@meshsdk/core": "1.9.0-beta.72",
    "@emurgo/cardano-serialization-lib-nodejs": "^15.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.13.5",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.38.0"
  }
}
