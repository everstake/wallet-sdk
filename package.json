{
  "name": "@everstake/wallet-sdk",
  "version": "1.0.19",
  "description": "Everstake Wallet SDK for Javascript",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run prebuild && tsup src/index.ts --format cjs,esm --dts",
    "type-check": "tsc",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "prebuild": "pnpm run type-check && pnpm run lint && pnpm run test"
  },
  "keywords": [
    "ethereum",
    "aptos",
    "polygon",
    "cosmos",
    "solana",
    "blockchain",
    "everstake",
    "wallet sdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/everstake/wallet-sdk.git"
  },
  "author": "Everstake",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/everstake/wallet-sdk/issues"
  },
  "homepage": "https://github.com/everstake/wallet-sdk#readme",
  "dependencies": {
    "@solana/web3.js": "1.98.2",
    "bignumber.js": "^9.3.0",
    "ethereum-multicall": "^2.26.0",
    "superstruct": "2.0.2",
    "web3": "4.16.0"
  },
  "overrides": {
    "ws": "^8.0.0",
    "elliptic": "6.6.1",
    "base-x": "3.0.11"
  },
  "pnpm": {
    "overrides": {
      "ws": "^8.0.0",
      "elliptic": "6.6.1",
      "base-x": "3.0.11"
    }
  },
  "resolutions": {
    "elliptic": "6.6.1",
    "base-x": "3.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.12",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.38.0"
  }
}
